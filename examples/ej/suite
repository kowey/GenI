v_runs 
semantics: [ name(s1 a vincent)
             run(e a) ]

v_loves_m 
semantics: [ name(s1 a vincent)
             love(e a ex)
             name(s2 ex mia)]

v_loves_é
semantics: [ name(s1 a vincent)
             love(e a ex)
             name(s2 ex émilie)]

v_loves_m_f
semantics : [name(s1 a vincent)
             love(e a ex)
             fast(s3 e)
             name(s2 ex mia)]

man_love_m
semantics: [man(s1 a) 
            love(e a ex)
            name(s2 ex mia)]

man_remove_rabbit
semantics : [man(s1 a) 
             remove(e a ex h)
	     rabbit(s2 ex)
	     hat(s3 h)]

man_remove_rabbit_fast
semantics : [man(s1 a) 
             remove(e a ex h)
	     fast(s4 e)
	     rabbit(s2 ex)
	     hat(s3 h)]

man_remove_rabbit_fast_def
semantics : [man(s1 a)
	     def(a) 
             remove(e a ex h)
	     fast(s4 e)
	     rabbit(s2 ex)
	     def(ex)
	     white(s4 ex)
	     def(h)
	     hat(s3 h)]

